<?xml version="1.0" encoding="UTF-8" ?>
  <XMLDB PATH="mod/sna/db" VERSION="20140704" COMMENT="XMLDB file for Moodle mod/sna">
    <TABLES>
      <TABLE NAME="sna" COMMENT="Default comment for sna, please edit me">
		<FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="course"/>
